{
  "name": "user-management-service",
  "version": "1.0.0",
  "description": "User Management Service with Express, PostgreSQL and TypeORM",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "node --watch src/app.js",
    "typeorm": "typeorm",
    "migration:generate": "npm run typeorm -- migration:generate -d src/config/data-source.js",
    "migration:run": "npm run typeorm -- migration:run -d src/config/data-source.js",
    "migration:revert": "npm run typeorm -- migration:revert -d src/config/data-source.js"
  },
  "keywords": [
    "express",
    "javascript",
    "typeorm",
    "postgresql",
    "user-management"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "typeorm": "^0.3.17",
    "pg": "^8.11.3",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "class-validator": "^0.14.0",
    "class-transformer": "^0.5.1",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5"
  }
}

